{
  "name": "notion-api-learn",
  "version": "1.0.0",
  "author": "jackluson <18219112108@163.com>",
  "description": "notion api learn and pratice",
  "keywords": [
    "notion",
    "api",
    "learn"
  ],
  "homepage": "https://github.com/jackluson/notion-api-learn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackluson/notion-api-learn.git"
  },
  "bugs": {
    "url": "https://github.com/jackluson/notion-api-learn/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier src/**/*.ts --write",
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "test": "jest --no-cache --coverage -u",
    "run:build": "node dist/index.js",
    "precommit": "lint-staged -q"
  },
  "dependencies": {
    "@notionhq/client": "^0.1.8",
    "@types/module-alias": "^2.0.0",
    "dotenv": "^10.0.0",
    "https-proxy-agent": "^5.0.0",
    "module-alias": "^2.2.2",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.3.1",
    "git-cz": "^4.7.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json}": "prettier --write",
    "src/**/*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
